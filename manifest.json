{
    "manifest_version": 3,
    "name": "Get Away From Bilibili",
    "version": "1.0",
    "description": "Close all Bilibili tabs within one click / 一键关闭所有Bilibili标签页",
    "permissions": [
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }